@model IEnumerable<WebApplication4.Dto.PatchDto>







<h1 style="text-align:right" class="page-title">عرض جميع - <span style="text-align:right" class="fw-semi-bold">الفرق</span></h1>




<section class="widget">
    <header>

        <div class="widget-controls">
            <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-chevron-up"></i></a>
            <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-chevron-down"></i></a>
            <a data-widgster="close" title="Close" href="#"><i class="glyphicon glyphicon-remove"></i></a>
        </div>
    </header>
    <div class="widget-body"  style="direction:rtl">

        <div class="mt">
            <table id="datatable-table" class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th style="text-align:right">رقم الفرقة</th>
                        <th style="text-align:right">اسم الفرقة</th>
                       




                    </tr>
                </thead>
                <tbody>


                    @foreach (var item in Model)
                    {
                    <tr>

                        <td><span class="fw-semi-bold">@item.PatchNumber</span></td>
                        <td class="hidden-sm-down">
                         
                            <br>
                         
                                <span class="fw-semi-bold">@item.PatchName</span>
                             
                        </td>
                   
                    </tr>




                    }
                </tbody>
            </table>
        </div>
    </div>
</section>